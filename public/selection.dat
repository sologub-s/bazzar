  answerIds.push($(this).val());
});

if(answerIds.length>0)
{
$.ajax({
method: "POST",
url: "/_data/_ajax/process_poll.php",
data: {questId: $('#questId').val(), answerId: answerIds},
success: function(data){
data = $.parseJSON(data);

if (data.status)
{
$('#poll_form').html(data.html);
}
else
{
$('#poll_form').parent('.form-group').removeClass('has-danger');
$('#poll_form .text-danger').addClass('hide');
}
}
})
}
else
{
$('#poll_form').parent('.form-group').addClass('has-danger');
$('#poll_form .text-danger').removeClass('hide');
}

return false;
});
}
</script>
